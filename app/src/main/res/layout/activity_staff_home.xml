<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StaffUI.StaffHomeActivity">


    <FrameLayout
        android:id="@+id/fragment_container_home_staff"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/bottom_navigation_staff_home"
        />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:padding="6dp"
        android:id="@+id/bottom_navigation_staff_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:menu="@menu/staff_bottom_navigation"
        android:background="#F4F4F4"
        />

</RelativeLayout>